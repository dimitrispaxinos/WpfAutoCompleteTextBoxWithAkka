<Application x:Class="WpfAutoCompleteTextBoxWithAkka.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:viewModels="clr-namespace:WpfAutoCompleteTextBoxWithAkka.ViewModels"
             xmlns:converters="clr-namespace:WpfAutoCompleteTextBoxWithAkka.Converters"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <viewModels:MainViewModel x:Key="MainViewModel"/>
        <converters:BoolToVisibilityConverter x:Key="BoolToVisibilityConverter"/>
        <converters:NullToFalseConverter x:Key="NullToFalseConverter"/>
        <converters:NullToTrueConverter x:Key="NullToTrueConverter"/>
        <converters:BooleanAndConverter x:Key="BooleanAndConverter"/>
    </Application.Resources>
</Application>
